{
  "title": "Chronicle of Wuhan Virus",
  "author": "Pyrhae",
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "-sharing",
    "github-buttons",
    "editlink",
    "ga",
    "-highlight",
    "prism",
    "bibtex-indexed-cite",
    "anchor-navigation-ex",
    "page-footer-ex",
    "splitter",
    "todo",
    "copy-code-button",
    "expandable-chapters-small",
    "donate",
    "flexible-alerts"
  ],
  "pluginsConfig": {
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord": ["Gitbook Use"]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "github-buttons": {
      "buttons": [{
        "user": "riskers",
        "repo": "gitbook-template",
        "type": "star",
        "size": "small",
        "count": true
      }]
    },
    "editlink": {
      "base": "https://github.com/pyrhae/covid-19/tree/master",
      "label": "Edit",
      "multilingual": true
    },
    "ga": {
      "token": "UA-131910384-3"
    },
    "prism": {
      "ignore": [
        "mermaid",
        "eval-js"
      ],
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ]
    },
    "page-footer-ex": {
      "copyright": "Released under [Attribution-ShareAlike 4.0 International License](https://creativecommons.org/licenses/by-sa/4.0/)",
      "markdown": true,
      "update_label": "This page last updated：",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "bibtex-indexed-cite": {
      "path": "/"
    },
    "donate": {
      "wechat": "https://pyrhae.github.io/covid-19/wechat.png",
      "alipay": "https://pyrhae.github.io/covid-19/alipay.jpg",
      "title": "",
      "button": "Donate",
      "alipayText": "支付宝打赏",
      "wechatText": "微信打赏"
    },
    "flexible-alerts": {
      "style": "flat",
      "comment": {
        "label": "Comment",
        "icon": "fa fa-comments",
        "className": "info"
      }
    }
  }
}
